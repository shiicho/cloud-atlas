# =============================================================================
# Staging 环境变量值
# =============================================================================
# Staging 环境更接近 prod，启用版本控制
# 生命周期比 dev 长，但比 prod 短
# =============================================================================

environment       = "staging"
bucket_prefix     = "myapp"
enable_versioning = true   # 与 prod 相同
lifecycle_days    = 30     # 比 dev 长，比 prod 短
